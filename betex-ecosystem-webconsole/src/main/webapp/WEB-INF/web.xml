<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>GWT-Maven-Archetype</display-name>

	<!-- Servlets -->
	<servlet>
		<servlet-name>MarketTradedVolume</servlet-name>
		<servlet-class>dk.betex.ecosystem.webconsole.server.MarketTradedVolumeServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MarketTradedVolume</servlet-name>
		<url-pattern>betexecosystem/MarketTradedVolume</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
