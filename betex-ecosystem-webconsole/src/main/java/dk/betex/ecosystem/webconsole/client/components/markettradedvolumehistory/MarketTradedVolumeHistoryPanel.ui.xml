<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gext="urn:import:com.google.gwt.widgetideas.client">

	<g:VerticalPanel ui:field="mainPanel" width="100%">
		<g:Label text="Speed" />
		<g:TextBox ui:field="playSpeed" text="20" />
		<g:Button ui:field="playButton" />
		<g:Label ui:field="errorLabel" />
		<g:Label ui:field="legendLabel" />
		<gext:SliderBar ui:field="slider" stepSize="1.0" />
	</g:VerticalPanel>

</ui:UiBinder> 